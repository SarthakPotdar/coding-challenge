#include <iostream>
#include <cstring>
#include <vector>
using namespace std;
int main()
{
int t;
cin>>t;
char c;
cin.get(c);
while(t--)
{
char arr1[10002],arr2[101000];
cin>>arr1>>arr2;
int lp = strlen(arr1);
int lh = strlen(arr2);
int p_abc[26] = {0};
int f = 0;
for(int i=0;i<lp;i++)
p_abc[arr1[i]-'a']++;
int checker_abc[26] = {0};
for(int i=0;i<lp;i++)
{
checker_abc[arr2[i]-'a']++;
}
for(int i=lp;i<lh;i++)
{
int found = 1;
for(int k=0;k<26;k++)
{
if(p_abc[k] != checker_abc[k])
{
found = 0;break;
}
}
if(found)
{
f=1;break;
}
checker_abc[arr2[i-lp]-'a']--;
checker_abc[arr2[i]-'a']++;
}
int found = 1;
for(int k=0;k<26 && !f;k++)
{
if(p_abc[k] != checker_abc[k])
{
found = 0;break;
}
}
if(found) f=1;
if(!f)
cout<<"NO"<<endl;
else
cout<<"YES"<<endl;
}
return 0;
}
